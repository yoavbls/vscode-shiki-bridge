{
  "name": "vscode-shiki-bridge-example-extension",
  "publisher": "vscode-shiki-bridge",
  "displayName": "extension-example",
  "description": "Use vscode-shiki-bridge package in web view panel",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.cjs",
  "contributes": {
    "commands": [
      {
        "command": "vscode-shiki-bridge-example-extension.shikiPreview",
        "title": "Shiki Preview",
        "category": "VS Code Shiki Bridge"
      },
      {
        "command": "vscode-shiki-bridge-example-extension.inspectInternals",
        "title": "Inspect Internals",
        "category": "VS Code Shiki Bridge"
      }
    ]
  },
  "dependencies": {
    "shiki": "^3.9.2",
    "vscode-shiki-bridge": "*"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node esbuild.cjs",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.cjs --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.cjs --production",
    "pretest": "npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "echo 'No tests configured'",
    "ci": "npm run check-types && npm run lint"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.6.3",
    "typescript": "^5.9.2"
  }
}